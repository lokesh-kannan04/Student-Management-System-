     <style >
        body{
            border: 0;
            padding: 0;
           
        }
        .contain
        {
            min-height: 100vh;
            display: flex;
            flex-direction: column;
            height: 100%;
          
        }
        .header
        {
            background-color: #2D4356;
            flex-basis: 3rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
            position: sticky;
            top: 0;
            transition: top 0.3s ease;
        }
        .header div
        {
            
            display: flex;
            justify-content: flex-end;
            align-items: center;
            height: 100%;
            padding-left: 20px;
            padding-right: 20px;
        }
        .header div :hover
        {
            background-color:#001524;
        }
        .head1
        {
            display: flex;
            justify-content: space-between;
            margin-right: 3%;
        
        }
     
        .header a
        {
            color: #f5e8d3;
            font-size: 20px;
            text-decoration: none;
        }
        .content
        {
            background-color: #f5e8d3;
            flex-basis: 35rem;
            display: flex;
            gap: white;
            justify-content: space-evenly;
            flex-wrap: wrap;
            align-content: space-around;
        }
        .logo
        {
            flex-basis: 30em;
            padding: 1rem;
          
        }
        .logodescription
        {
            flex-basis:30em ;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            
        }

        .footer
        {
            background-color:#2D4356;
            flex-basis:9rem;
        }
        footer
        {
            display: flex;
        }
        .contact
        {
            display: flex;
            gap: 20px;
            
        }
        .contact a
        {
            color: #f5e8d3;
        }
        .contact a:hover
        {
            color: #001524;
        }
        .homebuttons
        {
            display: flex;
            gap: 20px;
        }

