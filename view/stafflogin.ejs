    <script>
      var wasCorrect = "<%= locals.wrongup %>";
      console.log(wasCorrect);
      
      if(wasCorrect == 'false')
      {
        document.getElementById('wrongmsg').innerHTML = "*Roll number or Password is incorrect";
      }
      function myFunction() {
  var x = document.getElementById("myInput");
  if (x.type === "password") {
    x.type = "text";
  } else {
    x.type = "password";
  }
}
function toggleVisibility() {
        var eyeSlashIcon = document.querySelector('.notshow');
        var eyeIcon = document.querySelector('.show');

        if (eyeSlashIcon.style.display === 'none') {
            eyeSlashIcon.style.display = 'inline-block';
            eyeIcon.style.display = 'none';
        } else {
            eyeSlashIcon.style.display = 'none';
            eyeIcon.style.display = 'inline-block';
        }
    }
    </script>
